function Call_directives {
{% for condition in conditions %}
  $system_classes.SetCondition({{ condition|canonify_condition }})
{% endfor %}

  {{ bundle_name }} -ReportId "test@@0@@test" -TechniqueName "{{ technique_name }}" {{ params }} -PolicyMode ([Rudder.PolicyMode]::{{ policy_mode }}) -DirectiveId "{{ directive_id }}"
}
