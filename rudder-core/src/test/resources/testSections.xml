			<INPUT>
				<NAME>D</NAME>
				<DESCRIPTION>...</DESCRIPTION>
			</INPUT>
		</SECTION>
		<SECTION name="sect3" displayPriority="low">
			<INPUT>
				<NAME>E</NAME>
				<DESCRIPTION>...</DESCRIPTION>
			</INPUT>
		</SECTION>
		<SECTION name="emptySect">
		</SECTION>
	</SECTION>
  
  <SECTION name="component" component="true" componentKey="C" multivalued="true"/>
  
